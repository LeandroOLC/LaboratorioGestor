@model LaboratorioGestor.App.ViewModels.DentistaViewModel;

<div class="form-row">
    <div class="form-group col-md-9">
        <label asp-for="Enderecos.Endereco" class="control-label"></label>
        <input asp-for="Enderecos.Endereco" class="form-control" />
        <span asp-validation-for="Enderecos.Endereco" class="text-danger"></span>
    </div>
    <div class="form-group col-md-3">
        <label asp-for="Enderecos.Numero" class="control-label"></label>
        <input asp-for="Enderecos.Numero" class="form-control" />
        <span asp-validation-for="Enderecos.Numero" class="text-danger"></span>
    </div>
    <div class="form-group col-md-6">
        <label asp-for="Enderecos.Bairro" class="control-label"></label>
        <input asp-for="Enderecos.Bairro" class="form-control" />
        <span asp-validation-for="Enderecos.Bairro" class="text-danger"></span>
    </div>
    <div class="form-group col-md-4">
        <label asp-for="Enderecos.Cidade" class="control-label"></label>
        <input asp-for="Enderecos.Cidade" class="form-control" />
        <span asp-validation-for="Enderecos.Cidade" class="text-danger"></span>
    </div>
    <div class="form-group col-md-2">
        <label asp-for="Enderecos.UF" class="control-label"></label>
        <input asp-for="Enderecos.UF" class="form-control" />
        <span asp-validation-for="Enderecos.UF" class="text-danger"></span>
    </div>
    <div class="form-group col-md-12">
        <label asp-for="Enderecos.ReferenciaDoEndereco" class="control-label"></label>
        <input asp-for="Enderecos.ReferenciaDoEndereco" class="form-control" />
        <span asp-validation-for="Enderecos.ReferenciaDoEndereco" class="text-danger"></span>
    </div>
</div>